
/**
 * Constructor for objects in game
 * Jacob Peterson
 */
public class GameObject
{
    int xVel, yVel;
    int xPos, yPos;
    int width, height;
    
    public GameObject()
    {
        xPos = 0;
        yPos = 0;
    }
    
    public void setPosition(int x, int y)
    {
        this.xPos = x;
        this.yPos = y;
    }
    
    public void setVelocity(int x, int y)
    {
        this.xVel = x;
        this.yVel = y;
    }
    
    
}
